<script>
    // Rediects to HTTPS connection
    const restrictedOrigins = [
        "http://sergsol.dev",
        "https://solovevserg.github.io",
        "http://solovevserg.github.io",
        "http://sergsol.com",
        "https://sergsol.com",
        "http://sergsol.ru",
        "https://sergsol.ru",
    ];

    const target = 'https://solovevserg.github.io';

    if (restrictedOrigins.includes(location.origin)) {
        const href = location.href.replace(location.origin, target);
        console.log('Redirecting to', href);
        this.location.replace(href);
    }
</script>